{
  "license": "MIT",
  "name": "dme-browser",
  "productName": "dME Browser",
  "version": "1.1.17",
  "author": "dme network <cc@dme.network>",
  "description": "dME Browser",
  "private": true,
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=20.17.0 <21.0.0"
  },
  "packageManager": "yarn@1.22.22",
  "main": "main.js",
  "scripts": {
    "dev:next": "next dev -p 3003",
    "build:next": "next build",
    "start:next": "next start",
    "serve:next": "npx serve out",
    "start:electron": "wait-on tcp:3003 && cross-env ELECTRON_MODE=start electron .",
    "start-only:electron": "cross-env ELECTRON_MODE=start electron .",
    "build:electron": "node electron-builder.js",
    "dev": "concurrently \"npm run dev:next\" \"npm run start:electron\"",
    "build": "npm run build:next && npm run build:electron",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/inter": "^5.1.0",
    "@fontsource/roboto": "^5.0.15",
    "@mui/lab": "^6.0.0-beta.9",
    "@mui/material": "^6.0.2",
    "@mui/x-date-pickers": "^7.16.0",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "cookies-next": "^4.3.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^5.2.0",
    "electron-updater": "^6.3.9",
    "express": "^4.21.1",
    "lucide-react": "^0.446.0",
    "moment-timezone": "^0.5.45",
    "next": "14.2.11",
    "next-redux-wrapper": "^8.1.0",
    "pusher-js": "^8.4.0-rc2",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "sqlite3": "^5.1.7",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/electron": "^1.6.10",
    "@types/node": "^20",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "electron": "^32.0.2",
    "electron-builder": "^25.0.5",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8",
    "eslint-config-next": "14.2.8",
    "patch-package": "^8.0.0",
    "typescript": "^5",
    "wait-on": "^8.0.1"
  }
}